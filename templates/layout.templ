package templates

templ Layout(title string) {
	<!DOCTYPE html>
	<html lang="en">
	<head>
		<meta charset="UTF-8"/>
		<meta name="viewport" content="width=device-width, initial-scale=1.0"/>
		<title>{ title } - Go-Vault</title>
		<script src="https://cdn.tailwindcss.com"></script>
	</head>
	<body class="bg-white text-black font-mono">
		{ children... }
	</body>
	</html>
}
